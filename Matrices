#include<stdio.h>

int main(){
    int i=0,j=0;

    //Matriz con valores asignados
    int Matriz [3][3] = {{1,2,3},{1,2,3},{1,2,3}};

    //Matriz con valores ingresados por el usuario
    /*int Matriz [3][3];*/

    printf("Ingrese los valores para la Matriz/Tabla de 3 x 3\n\n");
 
   //Ciclo de llenado de la matriz
    for(i=0;i<3;i++){
            
        for(j=0;j<3;j++){
            
            printf("Elemento[%d, %d] : ", i, j);
            /*scanf("%d\n", &Matriz[i][j]);*/
            printf("{| %d |\n}",Matriz[i][j]);
        }
    }


return 0;
}

	
	
